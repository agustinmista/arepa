(module let3
  (val main
    (print_int (foo 1 2)))

  (fun foo (x y)
    (+ y
       (* x
          (+  (let ((a 10) (b 20))
                (* x (+ a b)))
              (letrec ((a 20) (b (+ a a)) (c (* a a)) (d (+ x y)))
                (+ a (- b (* d (+ x y)))))))))

  (fun print_int (x) (__prim_print_int__ x))
